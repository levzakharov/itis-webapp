<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="initial-schema-events-users-0" author="al.leontyev">
        <createTable tableName="events_users">
            <column name="user_id" type="bigint"/>
            <column name="event_id" type="bigint"/>
        </createTable>
    </changeSet>

    <changeSet id="initial-schema-events-users-1" author="al.leontyev">
        <addPrimaryKey tableName="events_users"
                       columnNames="user_id, event_id"
                       constraintName="events_users_pk"/>
    </changeSet>
</databaseChangeLog>